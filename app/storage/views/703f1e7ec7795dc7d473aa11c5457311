<script>

    $(function(){
        $.taxform.init();
        globalJs.Global.init();
        var target_class = "";
        var target = "";
        <?php if (Input::has('edit_action')) : ?>
            target_class = ".edit_quick_action";
            target = "<?php echo Input::get('edit_action') ?>";
        <?php else : ?>
            <?php if (Input::has('delete_action')) : ?>
                target_class = ".delete_quick_action";
                target = "<?php echo Input::get('delete_action') ?>";
            <?php endif; ?>
        <?php endif; ?>
        $.taxform.triggerQuickAction( $(target_class+'[data-targetitem="'+target+'"]') );
    });

</script>
